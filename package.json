  "description": "Geolocation suggestions and coordinates with Google Maps API for React",
  "main": "./lib/react-geoinput.js",
  "scripts": {
    "start": "npm run demo",
    "build": "webpack --config webpack.config.js --progress --colors",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "npm run test -- --singleRun=false",
    "lint": "eslint --ext .jsx ./src ./demos",
    "demo": "webpack-dev-server --config webpack.demo.config.js --content-base demo/"
  },
  "author": "nygardk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/woltapp/react-geoinput"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "geo",
    "geocoding",
    "geodestination",
    "geolocation",
    "input",
    "google",
    "maps"
  ],
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "prop-types": "^15.5.10",
